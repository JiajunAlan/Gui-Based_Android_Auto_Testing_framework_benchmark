                    <node index="27" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc='The current WGS84 and related information can now be displayed from the "GPS" menu, the displayed information will automatically update once a second as long as the dialog is shown. As a replacement for the old "create node at current location" functionality such a node can now be created from the dialog.' checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,4814][1060,5014]" />
                    <node index="28" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Styling for validation errors Heading" checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,5048][1060,5098]" />
                    <node index="29" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Every Vespucci user has seen the violet highlighting that indicates that an object may have an issue. Starting with this release the highlighting style can be configured in a limited fashion, see https://github.com/MarcusWolschon/osmeditor4android/blob/master/documentation/docs/tutorials/data_styling.md The default styles use this to highlight potentially out of date objects in orange." checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,5132][1687,5371]" />
                    <node index="30" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Upload selection and update data functions Heading" checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,5405][1060,5455]" />
                    <node index="31" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="It is now possible to upload only the currently selected objects, useful for example if you want to save part of an ongoing larger editing session. Further you can now update already downloaded data in place without effecting unsaved edits." checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,5489][1060,5649]" />
