                    <node index="2" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="End of support for pre Android 4.0 devices Heading" checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,449][1060,499]" />
                    <node index="3" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc='Version 15 no longer supports devices with Android versions older than 4.0 / API 14. We&apos;ve now migrated the Vespucci code base to use the "androidx" libraries to provide backwards compatibility over multiple Android versions and there is no feasible way to continue to use the old support libraries that would be necessary for older Android versions.' checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,533][1060,732]" />
                    <node index="4" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,761][1060,961]">
                      <node index="0" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Important note: even though the app will work on Android 4.0 devices, devices with Android version prior to 4.1 do not have TLS 1.2 support that is required for accessing the OpenStreetMap API since August 2nd 2020. If you are experiencing authorization issues on Android versions between 4.1 and 4.4 see " checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,761][1052,961]" />
                      <node index="1" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Can't (re-)authenticate - TLS 1.0 / 1.1 issues Link" checkable="false" checked="false" clickable="true" enabled="true" focusable="true" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[241,919][858,961]" />
                      <node index="2" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="." checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[855,919][866,961]" />
                    </node>
                    <node index="5" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Support for array like semantics for multi-selects Heading" checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,995][1060,1045]" />
                    <node index="6" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="In some circumstances, tags with multiple objects have array-like semantics (that is a fixed number of elements) contrary to being variable length lists. A typical example are lane related tags, which contain the number of elements in the lanes (or lanes:forward and lanes:backward) tag. There is no practical difference for tags that need to have a value for every element, but in the case of tags that have free form, potentially empty values, for example destination:lanes, there was previously no way of indicating this in the preset." checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,1079][1060,1396]" />
                    <node index="7" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="The current support will add empty fields if the number of elements is lower than the required number, and highlight surplus ones if there are more than the relevant tag requires." checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,1425][1060,1546]" />
                    <node index="8" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Switch to JOSM imagery configuration as default Heading" checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,1580][1060,1630]" />
                    <node index="9" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc='As you may know, Vespucci has used the "Editor Layer Index" as the source of imagery configuration since version 0.9.4 released in March 2014. ELI was one of the few, if not the only, successful, cross editor projects that reduced the amount of work duplicated across editor development, not to mention reducing the effort by community members to contribute. It was used directly by a number of projects and there was even a degree of synchronisation with JOSM.' checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,1664][1060,1794]" />
                    <node index="10" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Unluckily the iD developers have forked their own branch of ELI potentially making ELI longer term untenable. However JOSMs imagery configuration has come a long way since 2014 and now has feature parity with ELI and the JOSM developers have been kind enough to provide their data in an ELI compatible format." checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,1971][1060,2171]" />
                    <node index="11" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Starting with version 15 we now use JOSMs background layer configuration as the default, manual updates can be pulled from the JOSM repository or you can continue use ELI. As updates replace the whole configuration with the exception of custom layers, this means you can effectively switch back to using ELI if necessary." checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,2200][1060,2399]" />
                    <node index="12" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Improved layer support Heading" checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,2433][1060,2483]" />
                    <node index="13" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="The internal layer support has been rewritten to allow multiple layers of the same kind for layers that support this functionality (background and overlay imagery layers, geojson layer). Layers can be moved up and down in the layer stack. It should be pointed out that adding imagery layers uses a lot of resources and should be used sparingly." checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,2517][1060,2717]" />
                    <node index="14" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Mapillary support Heading" checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,2751][1060,2801]" />
                    <node index="15" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="A Mapillary layer can now be added to the layer configuration. Clicking on a Mapillary image marker will open the sequence at that marker in a viewer, the forward - backward buttons will navigate along the sequence. On devices with Android 7.0 the viewer starts in a separate activity that can be shown at the same time as the map display if you add them to a multi/split window view." checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,2835][1060,3074]" />
                    <node index="16" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Mapillary images are cached on device, the size of the cache (default 100MB) can be set in the advanced preferences." checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,3103][1060,3184]" />
                    <node index="17" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Support for WMS endpoints Heading" checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,3221][1060,3271]" />
                    <node index="18" text="" resource-id="" class="android.view.View" package="de.blau.android" content-desc="Vespucci now has support for querying WMS servers and adding layers as custom imagery (similar to how this works in JOSM). Additional WMS servers over the pre-configured ones can be added manually. The functionality can be accessed via the layer dialog menus for the background and overlay layers." checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[21,3305][1060,3504]" />
